<div class="active tab-pane" id="activity">
  <div class="box-body table-responsive no-padding">
    <table id="kelas_siswa" class="table table-hover">
      <thead>
        <!-- <th class="text-center">Latiahan</th>
        <th class="text-center">UTS</th>
        <th class="text-center">UAS</th> -->
        <th class="text-center">Nilai</th>
        <th class="text-center">Keterangan</th>
        <th class="text-center">Status</th>
        <th class="text-center">Aksi</th>
      </thead>
      <tbody>
        <%if(nilai !== 0){%>
        <tr class="text-center">
          <!-- <td><%= nilai.latihan %></td>
          <td><%= nilai.uts %></td>
          <td><%= nilai.uas %></td> -->
          <td><%= nilai.nilai_akhir %></td>
          <td><%= nilai.ket %></td>
          <td>
            <%  if(nilai.status === "Nonactive"){ %>
            <small class="label bg-red"><%= nilai.status %></small>
            <% } else { %>
            <small class="label bg-green"><%= nilai.status %></small>
            <% } %>
          </td>
          <td>
            <button class="btn btn-danger btn-sm" onclick="delete_nilai(`<%= nilai.id %>`,`<%= nilai.SiswaId %>`)">
              <span class=" glyphicon glyphicon-remove"></span>
              Delete
            </button>
          </td>
        </tr>
        <%}%>
      </tbody>
    </table>
  </div>
</div>