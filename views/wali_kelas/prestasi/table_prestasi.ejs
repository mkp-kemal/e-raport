<table id="prestasi" class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>No</th>
      <th>Tahun</th>
      <th>Nama</th>
      <th>Jenis</th>
      <th>Keterangan</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i = 0; i < prestasi.length; i++ ) { %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= prestasi[i].Tahun.tahun %></td>
      <td><%= prestasi[i].Siswa.nama %></td>
      <td><%= prestasi[i].jenis %></td>
      <td><%= prestasi[i].ket %></td>
      <td>
        <button class="btn btn-danger btn-sm" onclick="delete_prestasi(`<%= prestasi[i].id %>`)">
          <span class=" glyphicon glyphicon-remove"></span>
          Delete
        </button>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>